{
  "name": "web-app-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --config nodemon.json ./src/main.ts",
    "inspect": "nodemon --config nodemon.json --inspect ./src/main.ts",
    "test": "npm run test:w -- --run",
    "test:w": "vitest --dir ./src/",
    "test:e2e": "npm run test:e2e:w -- --run",
    "test:e2e:w": "vitest -c ./tests/vitest.config.ts",
    "coverage": "vitest run -c ./tests/vitest.config.ts --coverage",
    "prisma:format": "npx prisma format --schema src/common/database/schema.prisma",
    "models:generate": "npx prisma generate --schema src/common/database/schema.prisma",
    "migration:create": "npx prisma migrate dev --schema src/common/database/schema.prisma --create-only",
    "migration:deploy": "npx prisma migrate deploy --schema src/common/database/schema.prisma",
    "migration:rollback": "npx prisma migrate resolve --schema src/common/database/schema.prisma",
    "seed": "ts-node seeds/seed.ts",
    "lint": "eslint -c .eslintrc.json src/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitest/coverage-c8": "^0.25.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "inversify": "^6.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "bcrypt": "^5.1.0",
    "inversify-inject-decorators": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "prisma": "^4.6.1",
    "uuid": "^9.0.0"
  }
}
